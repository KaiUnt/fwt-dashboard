{
  "name": "fwt-dashboard-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build && node scripts/fix-sw.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:check": "npm run type-check && npm run lint && npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:manual": "echo 'Start dev server manually: npm run dev' && echo 'Then run: npm test'",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-query": "^5.82.0",
    "@types/papaparse": "^5.3.16",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "papaparse": "^5.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
